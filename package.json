{
  "name": "rntemplate",
  "version": "1.0.0",
  "description": "A React Native template.",
  "keywords": [
    "react-native",
    "jest",
    "template",
    "android",
    "ios"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pushpender-singh-ap/react-native-template.git"
  },
  "author": "Pushpender Singh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pushpender-singh-ap/react-native-template/issues"
  },
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native run-android && npx react-native run-ios",
    "start:metro": "npx react-native start",
    "clean": "watchman watch-del-all && rm -rf package-lock.json && rm -rf node_modules && cd ios && rm -rf Podfile.lock && rm -rf Pods && cd .. && npm install && cd ios && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "axios": "^0.26.1",
    "moment": "^2.29.3",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-native": "0.68.1",
    "react-native-gesture-handler": "^2.4.0",
    "react-native-localization": "^2.3.1",
    "react-native-paper": "^4.12.0",
    "react-native-reanimated": "^2.7.0",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
